{
  "schemaVersion": 1,
  "id": "signmeup",
  "version": "${version}",

  "name": "Sign Me Up",
  "description": "A mod that adds more signs.",
  "authors": [
    "RedstoneParadox"
  ],
  "contact": {
    "homepage": "https://github.com/RedstoneParadox/Sign-Me-Up",
    "sources": "https://github.com/RedstoneParadox/Sign-Me-Up"
  },

  "license": "MIT",
  "icon": "assets/signmeup/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "io.github.redstoneparadox.signmeup.SignMeUp"
      }
    ]
  },
  "mixins": [
    "signmeup.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.8.7",
    "fabric": "*",
    "fabric-language-kotlin": "*",
    "minecraft": "1.17.x"
  }
}
